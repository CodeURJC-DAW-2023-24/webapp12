{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/frontend",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/webfonts"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/bootstrap/scss/bootstrap.scss",
              "../backend/src/main/resources/static/css/style.css",
              "src/styles.css",
              "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.1/css/all.min.css",
              "https://cdn.jsdelivr.net/npm/bootstrap@4.5.0/dist/css/bootstrap.min.css",
              "https://cdnjs.cloudflare.com/ajax/libs/MaterialDesign-Webfont/7.2.96/css/materialdesignicons.min.css",
              "https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css",
              "https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css",
              "https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css",
              "https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css"
              
            ],
            "scripts": [
              "../backend/src/main/resources/static/js/popper.min.js",
              "../backend/src/main/resources/static/js/range.js",
              "../backend/src/main/resources/static/js/main.js",
              "../backend/src/main/resources/static/js/jquery.js",
              "../backend/src/main/resources/static/js/bootstrap.bundle.js",
              "../backend/src/main/resources/static/js/jquery.easing.js",
              "../backend/src/main/resources/static/js/sb-admin-2.js",
              "../backend/src/main/resources/static/vendor/jquery/jquery.min.js",
              "../backend/src/main/resources/static/vendor/bootstrap/js/bootstrap.bundle.min.js",
              "../backend/src/main/resources/static/vendor/jquery-easing/jquery.easing.min.js",
              "../backend/src/main/resources/static/js/loadMoreReservations.js",
              "../backend/src/main/resources/static/js/jquery.min.js",
              "../backend/src/main/resources/static/js/jquery-migrate-3.0.1.min.js",
              "../backend/src/main/resources/static/js/popper.min.js",
              "../backend/src/main/resources/static/js/bootstrap.min.js",
              "../backend/src/main/resources/static/js/jquery.easing.1.3.js",
              "../backend/src/main/resources/static/js/jquery.waypoints.min.js",
              "../backend/src/main/resources/static/js/jquery.stellar.min.js",
              "../backend/src/main/resources/static/js/owl.carousel.min.js",
              "../backend/src/main/resources/static/js/jquery.magnific-popup.min.js",
              "../backend/src/main/resources/static/js/aos.js",
              "../backend/src/main/resources/static/js/jquery.animateNumber.min.js",
              "../backend/src/main/resources/static/js/bootstrap-datepicker.js",
              "../backend/src/main/resources/static/js/scrollax.min.js",
              "../backend/src/main/resources/static/js/google-map.js",
              "../backend/src/main/resources/static/js/main.js",
              "../backend/src/main/resources/static/js/loadReviews.js",
              "../backend/src/main/resources/static/js/loadHotels.js",
              "../backend/src/main/resources/static/js/captcha.js",
              "../backend/src/main/resources/static/js/Chart.js",
              "../backend/src/main/resources/static/js/loadMoreHotelsviewHotelsManager.js"
            ],
            "baseHref": "/new/"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "frontend:build:production"
            },
            "development": {
              "buildTarget": "frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
